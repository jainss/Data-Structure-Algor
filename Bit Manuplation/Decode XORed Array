TC :O(N)
SC : O(N)

class Solution {
public:
    vector<int> decode(vector<int>& encoded, int first) {
        vector<int>vec(encoded.size()+1);
        vec[0]=first;
        for(int i =0;i<encoded.size();i++){
            vec[i+1]= vec[i]^encoded[i];
        }
        return vec;
    }
};

https://leetcode.com/problems/decode-xored-array/description/
