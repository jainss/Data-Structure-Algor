class Solution {
public:
    int search(vector<int>& nums, int target) {
        int i=0,j=nums.size()-1;
        while(i<=j){
            int mid=(j+i)/2;
            if(nums[mid]==target){
                return mid;
            }
            if(nums[mid]<target){
                i=mid+1;
            }else{
                j=mid-1;
            }
        }
        return -1;    
    }
};

https://leetcode.com/problems/binary-search/
