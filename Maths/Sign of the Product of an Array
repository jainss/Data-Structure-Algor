class Solution {
public:
    int arraySign(vector<int>& nums) {
        int ans=0;
        for(int i=0;i<nums.size();i++){
           if(nums[i]<0){
               ans++;
           }
            if(nums[i]==0){
                return 0;
            }
        }
        if(ans%2!=0){
            return -1;
        }return 1;
        
    }
};

https://leetcode.com/problems/sign-of-the-product-of-an-array/
