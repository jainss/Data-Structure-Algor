class Solution {
public:
    bool isPalindrome(int f) {
        if(f<0){
            return false;
        }
        long long int n=0;
        long long int x=f;
        while(x!=0){
            n=n*10+x%10;
            x/=10;
        }
        if(n==f){
            return true;
        }
        return false;
    }
};

https://leetcode.com/problems/palindrome-number/ 
