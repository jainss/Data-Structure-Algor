class Solution{
  public:
    int cutRod(int price[], int n) {
       int dp[n+1][n+1];
       for(int i=0;i<n+1;i++){
        for(int j=0;j<n+1;j++){
            if(i==0 || j==0){
                dp[i][j]=0;
            }else if(i<=j){
                dp[i][j]=max(price[i-1]+dp[i][j-i],dp[i-1][j]);
            }else{
                dp[i][j]=dp[i-1][j];
            }
        }
       }
       return dp[n][n];
    }
};



https://practice.geeksforgeeks.org/problems/rod-cutting0840/1/
