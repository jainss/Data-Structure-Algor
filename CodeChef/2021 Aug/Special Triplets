#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int n,c=0;
	    cin>>n;
	    for(int i=1;i<=n;i++){
	        for(int j=i;j<=n;j+=i){
	            for(int z=i;z<=n;z+=j){
	                if(z%j==i){
	                    c++;
	                }
	            }
	        }
	    }
	    cout<<c<<endl;
	}
	return 0;
}



