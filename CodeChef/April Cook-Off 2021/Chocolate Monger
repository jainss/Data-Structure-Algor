#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long int t;
    cin>>t;
    while(t--){
        long long int n,x,c,f=0;
        cin>>n>>x;
        map<long long int,long long int>mp;
        for(int i=0;i<n;i++){
            cin>>c;
            mp[c]++;
        }
        for(auto i:mp){
            x=x-i.second+1;
            i.second=1;
            if(x<=0){
                f=1;
                break;
            }
        }
        if(f==1){
            cout<<mp.size()<<endl;
        }
        else{
            if(x>0){
                cout<<mp.size()-x<<endl;
            }
        }
    }
    return 0;
}
