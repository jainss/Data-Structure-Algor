#include<bits/stdc++.h>
using namespace std;
 int main(){
     int t;
     cin>>t;
     while(t--){
         int am,bm,cm,tm,a,b,c;
         cin>>am>>bm>>cm>>tm>>a>>b>>c;
         if((a+b+c)<tm){
             cout<<"NO"<<endl;
             continue;
         }
         else if(a<am||b<bm||c<cm){
             cout<<"NO"<<endl;
             continue;
         }
         else{
             cout<<"YES"<<endl;
             continue;
         }
     }
     return 0;
 }
