class Solution {
public:
    int maximum69Number (int num) {
        string ans=to_string(num);
        for(int i=0;i<ans.size();i++){
            if(ans[i]=='6'){
                ans[i]='9';
                break;
            }
        }
        return stoi(ans);
    }
};




https://leetcode.com/problems/maximum-69-number/description/
