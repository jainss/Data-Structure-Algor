class Solution {
public:
    vector<int> shuffle(vector<int>& nums, int n) {
        int i=0,j=nums.size()/2;
         vector<int>ans;
        while(i<nums.size()/2 and j<nums.size()){
            ans.push_back(nums[i]);
            ans.push_back(nums[j]);
            i++;
            j++;
        }
        return ans;
    }
};



https://leetcode.com/problems/shuffle-the-array/
