TC : O(N)
SC : O(1)

class Solution {
public:
    bool isOneBitCharacter(vector<int>& bits) {
        int i =0;
        bool ans = false;
        while(i<bits.size()-1){
            if(bits[i]==1){
                i+=2;
            }else{
                i++;
            }
        }
        return i== bits.size()-1;
    }
};

