#include <bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    vector<vector<int>>vec;
    for(int i=0;i<n;i++){
        vector<int>v1;
        for(int j=0;j<n;j++){
            int c;
            cin>>c;
            v1.push_back(c);
        }
        vec.push_back(v1);
    }
    for(int i=0;i<vec.size();i++){
        for(int j=0,g=i;g<vec.size();g++,j++){
            cout<<vec[j][g]<<endl;
        }
    }
    return 0;
    // for(int i=0;i<n;i++){
    //     for(int j=0;j<n;j++){
    //         cout<<vec[i][j]<<" ";
    //     }
    //     cout<<endl;
    // }
}
